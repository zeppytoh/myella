---
const { variant, size, class: className, ...rest } = Astro.props;
const xs = ["size-3"];
const sm = ["size-4"];
const base = ["size-5"];
const md = ["size-6"];
const lg = ["size-7"];
const xl = ["size-8"];
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class:list={[
    "icon",
    "icon-tabler",
    "icons-tabler-outline",
    "icon-tabler-arrow-alert-api-app",
    size === "xs" && xs,
    size === "sm" && sm,
    size === "base" && base,
    size === "md" && md,
    size === "lg" && lg,
    size === "xl" && xl,
    className,
  ]}
  {...rest}>
  <path
    stroke="none"
    d="M0 0h24v24H0z"
    fill="none"
  ></path><path d="M12 15h-6.5a2.5 2.5 0 1 1 0 -5h.5"></path><path
    d="M15 12v6.5a2.5 2.5 0 1 1 -5 0v-.5"></path><path
    d="M12 9h6.5a2.5 2.5 0 1 1 0 5h-.5"></path><path
    d="M9 12v-6.5a2.5 2.5 0 0 1 5 0v.5"></path>
</svg>
